{
    "builds" : [
        {
        "src": "jswebart/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLamdaSize": "15mb", "runtime": "python3.9"}
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    },
    {
        "buildCommand": "apt-get update && apt-get install -y postgresql-client && pip3 install --disable-pip-version-check --target . --upgrade -r requirements.txt"
      }
      
],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src" : "/(.*)",
            "dest": "jswebart/wsgi.py"
        }
    ]
}